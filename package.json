{
  "name": "pastanaga-angular",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build_lib": "ng build --prod pastanaga-angular",
    "postbuild_lib": "cp -R ./projects/pastanaga/src/lib/styles dist/pastanaga/lib/styles && cp -R ./projects/pastanaga/src/assets dist/pastanaga/lib/assets",
    "build_demo": "ng build --prod --base-href /pastanaga-angular/dist/pastanaga-app/",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --ci",
    "get_version": "cat ./projects/pastanaga/package.json | grep version  | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]'",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.0",
    "@angular/cdk": "~8.1.0",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~8.1.0",
    "@angular/forms": "~8.1.0",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "angular-svg-icon": "^7.2.1",
    "angular-traversal": "^1.2.8",
    "date-fns": "^2.9.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.0",
    "@angular/language-service": "~8.1.0",
    "@ngrx/store-devtools": "^8.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "24.0.21",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "mrs-developer": "^1.1.3",
    "ng-packagr": "^5.1.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
